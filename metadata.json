{
  "name": "simp-rsyslog",
  "version": "5.1.1",
  "author": "SIMP",
  "summary": "A puppet module to support RSyslog versions 7 and higher using new style RainerScript.",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-rsyslog",
  "project_page": "https://github.com/simp/pupmod-simp-rsyslog",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "logging",
    "rsyslog"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.9.0 < 5.0.0"
    },
    {
      "name": "simp/iptables",
      "version_requirement": ">= 4.1.4 < 5.0.0"
    },
    {
      "name": "simp/logrotate",
      "version_requirement": ">= 4.1.0 < 5.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 1.3.1 < 2.0.0"
    },
    {
      "name": "simp/pki",
      "version_requirement": ">= 4.2.4 < 5.0.0"
    },
    {
      "name": "simp/tcpwrappers",
      "version_requirement": ">= 4.1.0 < 5.0.0"
    },
    {
      "name": "simp/compliance_markup",
      "version_requirement": ">= 1.0.1 < 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
}
